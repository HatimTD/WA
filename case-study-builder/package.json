{
  "name": "case-study-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack -p 3010",
    "build": "prisma generate && next build --webpack",
    "start": "next start -p 3010",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-lists": "tsx prisma/seed-master-lists.ts",
    "db:seed-test": "tsx scripts/seed-database.ts",
    "vercel-build": "prisma generate && prisma db push --accept-data-loss && next build --webpack",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:smoke": "playwright test e2e/smoke.spec.ts",
    "test:e2e:security": "playwright test e2e/security.spec.ts",
    "test:e2e:share": "playwright test e2e/share-buttons.spec.ts",
    "test:e2e:ratelimit": "playwright test e2e/rate-limiting.spec.ts",
    "test:all": "npm run test && npm run test:e2e",
    "sbom:generate": "cyclonedx-npm --output-format JSON --output-file sbom/sbom.json",
    "sbom:generate:xml": "cyclonedx-npm --output-format XML --output-file sbom/sbom.xml",
    "security:sbom": "npm run sbom:generate && npm run sbom:generate:xml",
    "test:e2e:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:e2e:perf": "playwright test e2e/performance.spec.ts",
    "security:audit": "npm audit --json > security-audit.json && npm audit"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.0",
    "@logtail/node": "^0.5.6",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@sentry/nextjs": "^10.29.0",
    "@serwist/next": "^9.2.1",
    "@types/xlsx": "^0.0.35",
    "@vercel/analytics": "^1.6.1",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.4",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^2.8.0",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "glob": "^11.0.3",
    "idb": "^8.0.3",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.446.0",
    "next": "^16.0.8",
    "next-auth": "^5.0.0-beta",
    "next-cloudinary": "^6.17.4",
    "next-themes": "^0.4.6",
    "openai": "^4.104.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-email": "^5.0.3",
    "react-google-autocomplete": "^2.7.5",
    "recharts": "^2.15.4",
    "resend": "^6.4.2",
    "serwist": "^9.2.1",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.2",
    "vosk-browser": "^0.0.8",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@cyclonedx/cyclonedx-npm": "^4.1.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/google.maps": "^3.58.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.47",
    "prisma": "^6.19.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6"
  },
  "optionalDependencies": {
    "@next/swc-win32-x64-msvc": "^16.0.8",
    "@rollup/rollup-win32-x64-msvc": "^4.53.3"
  }
}
